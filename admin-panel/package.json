{
  "name": "zerda-admin-panel",
  "version": "2.0.0",
  "description": "Zerda Advanced Admin Panel - Multi-tenant Feature & Theme Management",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "esbuild src/App.jsx --bundle --outfile=public/bundle.js --loader:.jsx=jsx",
    "watch": "esbuild src/App.jsx --bundle --outfile=public/bundle.js --loader:.jsx=jsx --watch",
    "build:prod": "esbuild src/App.jsx --bundle --outfile=public/bundle.js --loader:.jsx=jsx --minify",
    "migrate": "node migrations/migrate.js",
    "seed": "node migrations/seed.js"
  },
  "keywords": [
    "zerda",
    "admin",
    "feature-flags",
    "configuration",
    "multi-tenant",
    "theme-management"
  ],
  "author": "COSMOS IT+",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^13.4.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
